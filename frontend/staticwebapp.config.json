{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif,ico,svg}", "/static/*"]
  },
  "responseOverrides": {
    "404": { "rewrite": "/index.html", "statusCode": 200 }
  },
  "globalHeaders": {
    "Content-Security-Policy": "frame-ancestors 'self' https://portafolio-yordansv.netlify.app;",
    "X-Content-Type-Options": "nosniff",
    "X-XSS-Protection": "1; mode=block"
  }
}
